    <ProjectReference Include="..\Enigmatry.Entry.HealthChecks\Enigmatry.Entry.HealthChecks.csproj" />
    <ProjectReference Include="..\Enigmatry.Entry.SwaggerSecurity\Enigmatry.Entry.Swagger.csproj" />
  </ItemGroup>

